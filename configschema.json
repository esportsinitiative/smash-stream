{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"useMockData": {
			"type": "boolean",
			"description": "Whether or not to use mock data instead of the real tracker API. If true, pulls from Lange's Dropbox.",
			"default": false
		},
		"twitter": {
			"type": "object",
			"properties": {
				"consumerKey": {
					"type": "string"
				},
				"consumerSecret": {
					"type": "string"
				},
				"accessTokenKey": {
					"type": "string"
				},
				"accessTokenSecret": {
					"type": "string"
				}
			},
			"required": [
				"consumerKey",
				"consumerSecret",
				"accessTokenKey",
				"accessTokenSecret"
			]
		}
	}
}
